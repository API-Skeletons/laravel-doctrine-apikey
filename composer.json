{
    "name": "api-skeletons/laravel-doctrine-apikey",
    "description": "API keys with scopes for Laravel Doctrine",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ApiSkeletons\\Laravel\\Doctrine\\ApiKey\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApiSkeletonsTest\\Laravel\\Doctrine\\ApiKey\\": "test/"
        }
    },
    "authors": [
        {
            "name": "Tom H Anderson",
            "email": "tom.h.anderson@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel-doctrine/orm": "^1.6"
    },
    "require-dev": {
        "doctrine/coding-standard": "^9.0",
        "doctrine/dbal": "^3.1.1",
        "orchestra/testbench": "^6.23",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.15"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ApiSkeletons\\Laravel\\Doctrine\\ApiKey\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpcs && vendor/bin/phpunit && vendor/bin/psalm"
    }
}
