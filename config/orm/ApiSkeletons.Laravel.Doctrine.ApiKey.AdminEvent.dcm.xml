<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ApiSkeletons\Laravel\Doctrine\ApiKey\AdminEvent" table="api_key_admin_events">
    <id name="id" type="bigint">
      <generator strategy="AUTO"/>
    </id>
    <field name="ip_address" type="string" nullable="false"/>
    <field name="event" type="string" nullable="false"/>
    <field name="created_at" type="datetime" nullable="false"/>
    <many-to-one field="apiKey" target-entity="ApiSkeletons\Laravel\Doctrine\ApiKey\ApiKey" inversed-by="adminEvents">
      <join-columns>
        <join-column name="api_key_id" referenced-column-name="id" nullable="false"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
